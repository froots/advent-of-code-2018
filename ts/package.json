{
  "name": "advent-of-code-2018",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node ./dist/scripts/solve.js",
    "test": "tsc && tape dist/**/*tests.js | tap-dot",
    "test:w": "tape-watch dist/**/*tests.js -o '| tap-dot --color'",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "devDependencies": {
    "@types/node": "^10.12.11",
    "@types/tape": "^4.2.32",
    "tap-dot": "^2.0.0",
    "tape": "^4.9.1",
    "tape-watch": "^2.3.0",
    "typescript": "^3.2.1"
  }
}
